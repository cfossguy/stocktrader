{
    "family": "jwilliams-stockpicker-datapipeline-def",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "2048",
    "executionRoleArn": "arn:aws:iam::461485115270:role/jwilliams-stockpicker",
    "containerDefinitions": [
      {
        "name": "stockpicker-container-ecs",
        "image": "461485115270.dkr.ecr.us-east-2.amazonaws.com/jwilliams-stockpicker-datapipeline:latest",
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/jwilliams-stockpicker-datapipeline",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }